<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/colorRateText"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vibrantzz3.snipsearchadmin.MainActivity">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorRateText"
        android:navigationIcon="@drawable/backarrow5"
        app:layout_collapseMode="pin"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

    </android.support.v7.widget.Toolbar>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="65dp"
        android:text="Select Gender"
        android:textColor="@android:color/black"
        android:textSize="14dp" />

    <ImageView
        android:id="@+id/femaleimg"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/femaleimg" />

    <ImageView
        android:id="@+id/femaleimgpress"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentStart="true"
        android:visibility="invisible"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/tick" />

    <TextView
        android:layout_below="@+id/femaleimg"
        android:layout_marginLeft="70dp"
        android:textSize="18dp"
        android:textColor="@color/colorGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="FEMALE"/>

    <!--<ImageView
        android:id="@+id/acadmeyimg"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="270dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/circle" />

    <TextView
        android:layout_below="@+id/acadmeyimg"
        android:layout_marginLeft="230dp"
        android:textSize="18dp"
        android:textColor="@color/colorGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="ACADMEY"/>-->

    <ImageView
        android:id="@+id/uniseximg"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="250dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/uniseximg" />

    <ImageView
        android:id="@+id/uniseximgpress"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="250dp"
        android:layout_alignParentStart="true"
        android:visibility="invisible"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/tick" />

    <TextView
        android:layout_below="@+id/uniseximg"
        android:layout_marginLeft="70dp"
        android:textSize="18dp"
        android:textColor="@color/colorGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="UNISEX"/>

    <ImageView
        android:id="@+id/maleimg"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/maleimg" />

    <ImageView
        android:id="@+id/maleimgpress"
        android:layout_width="159dp"
        android:layout_height="146dp"
        android:layout_marginLeft="190dp"
        android:layout_marginTop="50dp"
        android:layout_alignParentStart="true"
        android:visibility="invisible"
        android:layout_below="@+id/toolbar"
        android:src="@drawable/tick" />

    <TextView
        android:layout_below="@+id/maleimg"
        android:layout_marginLeft="240dp"
        android:textSize="18dp"
        android:textColor="@color/colorGrey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="MALE"/>

    <ImageView
        android:id="@+id/go"
        android:layout_width="132dp"
        android:layout_height="60dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="20dp"
        android:layout_marginLeft="120dp"
        android:onClick="GetEditfillform3a"
        android:src="@drawable/save1" />






</RelativeLayout>


