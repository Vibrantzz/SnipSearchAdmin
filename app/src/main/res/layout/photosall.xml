<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/colorRateText"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.vibrantzz3.snipsearch.ServiceAll">

    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorblue"
        android:navigationIcon="@drawable/backarrow3"
        app:layout_collapseMode="pin"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

    </android.support.v7.widget.Toolbar>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="508dp"
        android:layout_marginTop="60dp">

        <TextView
            android:id="@+id/textView13"
            android:layout_width="80dp"
            android:layout_height="25dp"
            android:layout_marginBottom="2dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="2dp"
            android:paddingBottom="5dp"
            android:text="Photos"
            android:textColor="@android:color/black"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/p1"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_below="@+id/textView13"
            android:layout_marginLeft="10dp"
            android:src="@drawable/picture1" />

        <ImageView
            android:id="@+id/p4"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/textView13"
            android:layout_marginEnd="10dp"
            android:src="@drawable/picture1" />


        <ImageView
            android:id="@+id/p3"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_below="@+id/textView13"
            android:layout_marginRight="5dp"
            android:layout_toStartOf="@+id/p4"
            android:src="@drawable/picture1" />

        <ImageView
            android:id="@+id/p2"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView13"
            android:layout_marginStart="98dp"
            android:src="@drawable/picture1" />

        <ImageView
            android:id="@+id/p5"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_below="@+id/p1"
            android:layout_marginLeft="10dp"
            android:src="@drawable/picture1" />

        <ImageView
            android:id="@+id/p8"
            android:layout_width="80dp"
            android:layout_height="90dp"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/p4"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/picture1" />


        <ImageView
            android:id="@+id/p6"
            android:layout_width="80dp"
            android:layout_height="97dp"
            android:layout_alignParentEnd="true"
            android:layout_below="@+id/p3"
            android:layout_centerVertical="true"
            android:layout_marginEnd="98dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/picture1" />

        <ImageView
            android:id="@+id/p7"
            android:layout_width="80dp"
            android:layout_height="97dp"
            android:layout_alignStart="@+id/p2"
            android:layout_below="@+id/p2"
            android:layout_marginTop="-2dp"
            android:src="@drawable/picture1" />

    </RelativeLayout>


</android.support.design.widget.CoordinatorLayout>